<script>
    //当前时间
    $(function () {
        var myDate = new Date;
        var year = myDate.getFullYear(); //获取当前年
        var mon = myDate.getMonth() + 1; //获取当前月
        var date = myDate.getDate(); //获取当前日
        var timeing = year + "-" + mon + "-" + date
        $("#stDate1").attr("value", timeing);
    });

</script>

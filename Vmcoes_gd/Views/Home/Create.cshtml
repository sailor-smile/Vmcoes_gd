                            <div class="dt-buttons btn-overlap btn-group">
                                <span class="gdxuanzhekuang" style="margin-left:60px;">
                                    请选择工地   <select id="wokao1" class="opt8" style="border-color: #e1e1e1;">
                                        <option value="">--请选择--</option>
                                    </select>
                                </span>
                            </div>


                            @*<div class="dt-buttons btn-overlap btn-group " style="margin-left:960px;">

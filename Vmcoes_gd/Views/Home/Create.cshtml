                    </div>
                    <div class="table-header">
                        信息录入添加区域
                    </div>
                    
                    <table id="indexTabHead" class="table  table-bordered table-hover"></table>
                    <table id="indexTab" class="table  table-bordered table-hover">
                        <thead>
                            <tr align="center">
                                <th>日期</th>
                                <th>时间</th>
                                <th>工地名称</th>
                                <th>工地地址</th>
                                <th>运输车辆</th>
                                <th>是否冲洗</th>
                                <th>是否密闭</th>
                                <th>是否办理有效处置证</th>
                                <th>运输公司名称</th>
                                <th>操作</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>

                    <div class="clearfix form-actions" id="option">
                        <div class="col-md-offset-5 col-md-6">
                            <button class="btn btn-info" type="button" id="btnSave1">
                                <i class="ace-icon fa fa-check bigger-110"></i>
                                提交
                            </button>

                            &nbsp; &nbsp; &nbsp;
                            <button class="btn" type="button" id="resetBtn">
                                <i class="ace-icon fa fa-undo bigger-110"></i>
                                重置
                            </button>
                        </div>
                    </div>

                </div>
            </div>

            <!-- PAGE CONTENT ENDS -->
        </div><!-- /.col -->
    </div><!-- /.row -->
</div><!-- /.page-content -->


